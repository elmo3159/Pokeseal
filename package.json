{
  "name": "pokeseal",
  "version": "1.0.0",
  "description": "ポケシル（ポケットシールちょう）- デジタルシール帳アプリ",
  "private": true,
  "scripts": {
    "predev": "node scripts/sync-stickers.mjs",
    "dev": "next dev",
    "prebuild": "node scripts/sync-stickers.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "analyze": "ANALYZE=true next build",
    "analyze:win": "set ANALYZE=true && next build",
    "stickers:generate": "node scripts/sync-stickers.mjs",
    "stickers:sync": "node scripts/sync-stickers.mjs --db",
    "mobile:build": "next build && npx cap sync",
    "mobile:ios": "next build && npx cap sync ios && npx cap open ios",
    "mobile:android": "next build && npx cap sync android && npx cap open android",
    "cap:sync": "npx cap sync",
    "cap:ios": "npx cap open ios",
    "cap:android": "npx cap open android"
  },
  "keywords": [
    "sticker",
    "album",
    "mobile",
    "app"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@capacitor/android": "^7.4.4",
    "@capacitor/cli": "^7.4.4",
    "@capacitor/core": "^7.4.4",
    "@capacitor/haptics": "^7.0.2",
    "@capacitor/ios": "^7.4.4",
    "@capacitor/keyboard": "^7.0.3",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/preferences": "^7.0.2",
    "@capacitor/push-notifications": "^8.0.0",
    "@capacitor/splash-screen": "^7.0.3",
    "@capacitor/status-bar": "^7.0.3",
    "@eslint/eslintrc": "^3.3.3",
    "@floating-ui/react": "^0.27.16",
    "@sentry/react": "^8.55.0",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.88.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tsparticles/react": "^3.0.0",
    "@tsparticles/slim": "^3.9.1",
    "capacitor-secure-storage-plugin": "^0.12.0",
    "framer-motion": "^12.23.25",
    "html-to-image": "^1.11.13",
    "html2canvas": "^1.4.1",
    "lottie-react": "^2.4.1",
    "lucide-react": "^0.555.0",
    "next": "^16.1.0",
    "next-pwa": "^5.6.0",
    "page-flip": "^2.0.7",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-pageflip": "^2.0.3",
    "react-virtuoso": "^4.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@next/bundle-analyzer": "^16.1.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "autoprefixer": "^10.4.22",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.6",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
